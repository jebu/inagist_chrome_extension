{
  "name": "inagist.com",
  "version": "1.0.0",
  "description": "inagist.com notification extension",
  "browser_action": {
    "default_icon": "img/inagist19.png"
  },
  "permissions": [
    "notifications",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "19": "img/inagist19.png",
    "32": "img/inagist32.png",
    "48": "img/inagist48.png"
  },
  "background_page":"background.html",
  "options_page":"options.html"
}
